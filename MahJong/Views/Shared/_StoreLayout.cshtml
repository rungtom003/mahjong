                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="Store" asp-action="Order" class="nav-link">
                                <i class="nav-icon fas fa-jedi-order"></i>
                                <p>เพิ่มแอดมินร้าน</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="Store" asp-action="Logout" class="nav-link">
                                <i class="nav-icon fas fa-jedi-order"></i>
                                <p>ออกจากระบบ</p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>


        <!-- Content Wrapper. Contains page content -->
        @RenderBody()

        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>MAHJONG</strong>
            <div class="float-right d-none d-sm-inline-block">
